{
    "name": "yeknava/laravel-idempotent",
    "description": "idempotent middleware for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "yeknava",
            "email": "yeknava.1@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">= 7.1",
        "ext-json": "*",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|6.*|7.*|8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.4.3",
        "fzaninotto/faker": "^1.4",
        "orchestra/testbench": "^5.0",
        "phpstan/phpstan": "^0.12"
    },
    "autoload": {
        "psr-4": {
            "Yeknava\\LaravelIdempotent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yeknava\\LaravelIdempotent\\LaravelIdempotentServiceProvider"
            ]
        }
    }
}
